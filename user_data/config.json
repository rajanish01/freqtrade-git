{
    "$schema": "https://schema.freqtrade.io/schema.json",
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "tradable_balance_ratio": 0.99,
    "dry_run": true,
    "dry_run_wallet": 1000,
    "cancel_open_orders_on_exit": true,
    "trading_mode": "futures",
    "margin_mode": "isolated",
    "unfilledtimeout": {
        "entry": 110,
        "exit": 180,
        "exit_timeout_count": 0,
        "unit": "minutes"
    },
    "order_types": {
        "entry": "limit",
        "exit": "market",
        "emergency_exit": "market",
        "stoploss": "market",
        "stoploss_on_exchange": true,
        "stoploss_on_exchange_interval": 5,
        "stoploss_price_type": "mark",
        "stoploss_on_exchange_limit_ratio": 0.99
    },
    "entry_pricing": {
        "price_side": "same",
        "use_order_book": true,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        }
    },
    "exit_pricing": {
        "price_side": "other",
        "use_order_book": true,
        "order_book_top": 1
    },
    "exchange": {
        "name": "binanceusdm",
        "key": "",
        "secret": "",
        "ccxt_config": {
            "options": {
                "defaultType": "future"
            },
            "enableRateLimit": true
        },
        "ccxt_async_config": {
            "options": {
                "defaultType": "future"
            },
            "enableRateLimit": true
        },
        "pair_whitelist": [
